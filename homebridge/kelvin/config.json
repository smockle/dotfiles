{
  "version": 1,
  "bridge": {
    "ip": "$IP_ADDRESS",
    "username": "$USERNAME"
  },
  "location": {
    "latitude": $LATITUDE,
    "longitude": $LONGITUDE
  },
  "webinterface": {
    "enabled": false,
    "port": 8080
  },
  "schedules": [
    {
      "name": "default",
      "associatedDeviceIDs": [
        1,
        2,
        4,
        5,
        6,
        11,
        12,
        13,
        14
      ],
      "enableWhenLightsAppear": true,
      "defaultColorTemperature": 4000,
      "defaultBrightness": 100,
      "beforeSunrise": [
        {
          "time": "4:00",
          "colorTemperature": 2200,
          "brightness": 60
        }
      ],
      "afterSunset": [
        {
          "time": "17:30",
          "colorTemperature": 2700,
          "brightness": 99
        },
        {
          "time": "20:00",
          "colorTemperature": 2300,
          "brightness": 80
        },
        {
          "time": "23:00",
          "colorTemperature": 2200,
          "brightness": 60
        }
      ]
    }
  ]
}
