// Place your key bindings in this file to overwrite the defaults
[
{ "key": "shift+cmd+o",                 "command": "workbench.action.quickOpen" },
{ "key": "cmd+\\",                      "command": "workbench.action.toggleSidebarVisibility" },
{ "key": "alt+cmd+w",                   "command": "workbench.action.closeOtherEditors", "when": "!searchInputBoxFocus || !searchViewletVisible" },
{ "key": "cmd+shift+[",                 "command": "workbench.action.previousEditor" },
{ "key": "cmd+shift+]",                 "command": "workbench.action.nextEditor" }
]